{
  "name": "ibrand/ec-open-core",
  "type": "library",
  "description": "GuoJiangClub EC's open source core.",
  "keywords": [
    "GuoJiangClub",
    "e-commerce",
    "open source",
    "server"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shjchen",
      "email": "ibrand.shjchen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "GuoJiangClub\\EC\\Open\\Core\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "GuoJiangClub\\EC\\Open\\Core\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
